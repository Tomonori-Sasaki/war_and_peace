	<script type="text/javascript">
		(function($, window, undefined) {
			// グローバル関数
			window.resizeImages = function()
			{
				$(".imagefit01, .imagefit02").imagefit({mode:"inside"});
			};
			window.sortElements = function()
			{
				var $wrapper = $("#sortitems")
				var $elements = $wrapper.children();

				var orders_visible   = [];
				var $elements_visible = {};

				$wrapper.hide();

				// 非表示要素を隠す
				$elements.filter("[_shown=0]")
					.hide();

				// 表示要素の順序を取得
				$elements.filter("[_shown=1]")
					.each(function()
					{
						var $element = $(this);
						var  order = $element.attr("_order");
						orders_visible.push(order);
						$elements_visible[order] = $element;
					})
					.show();

				// 順序をソート
				orders_visible.sort();
				for(var i = 0; i < orders_visible.length; i++)
				{
					var order = orders_visible[i];
					var $element = $elements_visible[order];
					$wrapper.append($element);
				}

				$wrapper.show();
			};
			window.logoChanged = function()
			{
				var $header_txt = $("#header_txt");
				var $header_img = $("#header_img");
				var src = $header_img.find("img").attr("src");
				if(src === "")
				{
					$header_txt.show();
					$header_img.hide();
				}
				else
				{
					$header_txt.hide();
					$header_img.show();
				}
			};

			$(function() {
				var $page_top = $('#page-top').css('bottom', '-100px');
				var showFlag = false;

				$(window)
					.on("load", function() {
						window.resizeImages();
					})
					.on("scroll", function () {
						//スクロール量が100に達したらボタン表示
						if ($(this).scrollTop() > 100) {
							if (showFlag == false) {
								showFlag = true;
								$page_top.stop().animate({'bottom' : '0px'}, "fast");
							}
						} else {
							if (showFlag) {
								showFlag = false;
								$page_top.stop().animate({'bottom' : '-100px'}, "fast");
							}
						}
					});

				// クリックされたらトップへスクロール
				$page_top.on("click", function () {
					$('body,html').animate({
						scrollTop: 0
					}, 500);
					return false;
				});

				window.sortElements();
				window.logoChanged();
			});
		})(jQuery, window);
	</script>
